    "isinteger",
    "is",
    "safe",
    "safeinteger",
    "issafe",
    "number"
  ],
  "bugs": {
    "url": "https://github.com/validate-io/safe-integer/issues"
  },
  "dependencies": {
    "compute-const-max-safe-integer": "^1.0.0",
    "compute-const-min-safe-integer": "^1.0.0",
    "validate.io-integer-primitive": "^1.0.0"
  },
  "devDependencies": {
    "chai": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "^1.0.0",
    "mocha": "2.x.x",
    "proxyquire": "^1.4.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
